{
    "name": "nmfmcosta/laravel-log-viewer",
    "description": "A Laravel log reader",
    "license": "MIT",
    "keywords": ["log", "log-reader", "log-viewer", "logging", "laravel", "lumen"],
    "type": "laravel-package",
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*|5.*|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7|^9.5.10",
        "orchestra/testbench": "3.7.*|^7.0"
    },
    "autoload": {
        "classmap": [
            "src/controllers"
        ],
        "psr-0": {
            "LB\\LaravelLogViewer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LB\\LaravelLogViewer\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LB\\LaravelLogViewer\\LaravelLogViewerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
